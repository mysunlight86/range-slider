{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:fsd/all"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "fsd"
    ],
    "rules": {
            "import/extensions": "off"
        },
        "settings": {
            "import/resolver": {
                "node": {
                    "extensions": [".js", ".ts"]
                }
            }
        }
}